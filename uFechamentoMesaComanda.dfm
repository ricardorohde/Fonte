  object qrVenda: TUniQuery
    SQLUpdate.Strings = (
      'UPDATE venda'
      'SET'
      
        '  ven_001 = :ven_001, emp_001 = :emp_001, nro_pessoas = :nro_pes' +
        'soas,'
      '  nro_couvert_m = :nro_couvert_m ,'
      '  nro_couvert_f = :nro_couvert_f '
      ''
      'WHERE'
      '  ven_001 = :Old_ven_001 AND emp_001 = :Old_emp_001')
    Connection = frmMenu.conexao
    SQL.Strings = (
      'select'
      'v.ven_001, '
      'm.mes_001 as id_mesa,'
      'v.ven_001 as id_venda,'
      'v.emp_001, '
      'm.mes_003 as nro_mesa, '
      'm.mes_002 as descricao_mesa, '
      'coalesce(v.ven_009,0) as valor_venda, '
      'v.nro_pessoas,'
      'v.nro_couvert_m,'
      'v.nro_couvert_f,'
      'v.cpf_cliente'
      'from venda v'
      '-- uniao com as vendas em aberto e pre fechadas situacoes 8 e 21'
      
        'left join  mesa m  on (m.emp_001 = v.emp_001) and (m.mes_001 = v' +
        '.ven_025) and m.sit_001 in (4, 19)'
      '--mesas ativas e reservadas'
      'where  v.sit_001 in (8,21)'
      'and v.emp_001 = :emp and '
      '(m.mes_003 = :nro_mesa or v.ven_026=:comanda)'
      'order by m.mes_003')
    AfterOpen = qrVendaAfterOpen
    BeforePost = qrVendaBeforePost
    Left = 346
    Top = 60
    ParamData = <
      item
        DataType = ftInteger
        Name = 'emp'
        Value = 1
